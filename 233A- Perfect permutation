//By ankitraturi44, contest: Codeforces Round #144 (Div. 2), problem: (A) Perfect Permutation, Accepted.


#include<bits/stdc++.h>
using namespace std;
int main()
{
    int n;
    cin>>n;
    if(n%2==1)
    {
        cout<<"-1\n";
    }
    else
    {
        cout<<"2 1 ";
        for(int i=3;i<n;i+=2)
        {
            cout<<i+1<<" "<<i<<" ";
        }
    }
    return 0;
}s
