#include<bits/stdc++.h>
#include<algorithm>
using namespace std;
int main()
{
    ios::sync_with_stdio(0);
    cin.tie(0);
    string s,d;
    cin>>s>>d;
    transform(s.begin(),s.end(),s.begin(),::tolower);
    transform(d.begin(),d.end(),d.begin(),::tolower);
 
        if(s>d)
        {
            cout<<"1";
        }
        else if(s<d)
        {
            cout<<"-1";
        }
        else if(s==d)
        {
            cout<<"0";
        }
   return 0;
}
