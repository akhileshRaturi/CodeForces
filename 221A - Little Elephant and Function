#include<bits/stdc++.h>
using namespace std;
int main()
{
    ios::sync_with_stdio(0);
    cin.tie(0);
    int n;
    cin>>n;
    cout<<n<<" ";
    for(int i=1;i<n;i++)
        cout<<i<<" ";
    return 0;
}
