#include <iostream>
#include<bits/stdc++.h>
using namespace std;
 
int main() {
	ios::sync_with_stdio(0);
	cin.tie(0);
	int n;
	cin>>n;
	int d=0;
	while(n--)
	{
		
		int a,b,c;
		cin>>a>>b>>c;
		if(a==1 && b==1 && c==1)
			d++;
		else if(a==1 && b==1)
				d++;
			else if(a==1 && c==1)
				d++;
				else if(b==1 && c==1)
					d++;
		
	}
	cout<<d<<endl;
	return 0;
}
