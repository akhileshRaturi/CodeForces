#include<bits/stdc++.h>
using namespace std;
int main()
{
    ios::sync_with_stdio(0);
    cin.tie(0);
    string s;
    cin>>s;
    string p;
    cin>>p;
    int n=p.size()-1;
    for(int i=0;i<s.size();i++)
    {
        if(s[i]==p[n])
        {
            n--;
        }
        else
        {
            cout<<"NO\n";
            return 0;
        }
    }
    cout<<"YES\n";
    return 0;
}
