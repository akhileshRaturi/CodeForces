#include<bits/stdc++.h>
using namespace std;
int main()
{
    ios::sync_with_stdio(0);
    cin.tie(0);
    string s,p="hello";
    int j=0,c=0;
    cin>>s;
    for(int i=0;i<s.size();i++)
    {
        if(s[i]==p[j])
        {
            j++;
            c++;
            if(c==5)
            {
                cout<<"YES\n";
                return 0;
            }
        }
    }
    cout<<"NO\n";
    return 0;
}
