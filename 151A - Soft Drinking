//By ankitraturi44, contest: Codeforces Round #107 (Div. 2), problem: (A) Soft Drinking, Accepted


#include<bits/stdc++.h>
using namespace std;
int main()
{
    ios::sync_with_stdio(0);
    cin.tie(0);
    
    int n,k,l,c,d,p,nl,np;
    
    cin>> n >> k >> l >> c >> d >> p >> nl >> np;
    
    int u=k*l;
    int q=u/nl;
    int r=c*d;
    int s=p/np;
    int jo=min(q,r);
    cout<<min(jo,s)/n;
    return 0;
}
