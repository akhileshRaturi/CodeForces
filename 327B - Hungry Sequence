//By ankitraturi44, contest: Codeforces Round #191 (Div. 2), problem: (B) Hungry Sequence, Accepted.


#include<bits/stdc++.h>
using namespace std;
int main()
{
    ios::sync_with_stdio(0),cin.tie(0);
    int n;
    cin>>n;
    int c=0;
    for(int i=1;c<n;i++)
    {
        cout<<n+i<<" ";
        c++;
    }
    cout<<"\n";
    return 0;
}
