//By ankitraturi44, contest: Codeforces Round #131 (Div. 2), problem: (A) System of Equations, Accepted.


#include<bits/stdc++.h>
using namespace std;
int main()
{
    ios::sync_with_stdio(0);
    cin.tie(0);
    int n,m;
    cin>>n>>m;
    int c=0;
    for(int i=0;i*i<=n && i<=m;i++)
    {
        int b=n-i*i;
        if(i+b*b==m)
            c++;
    }
    cout<<c<<endl;
    return 0;
}
